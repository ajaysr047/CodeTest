               <font>
                  <Font name="Verdana" size="15.0" />
               </font></JFXTextField>
            <JFXPasswordField fx:id="password" focusColor="#272727" labelFloat="true" layoutX="34.0" layoutY="261.0" prefHeight="26.0" prefWidth="266.0" promptText="Password" unFocusColor="#f4f4f4">
               <font>
                  <Font name="Verdana" size="15.0" />
               </font></JFXPasswordField>
            <JFXButton buttonType="RAISED" layoutX="34.0" layoutY="340.0" onAction="#handleLogin" prefHeight="31.0" prefWidth="266.0" style="-fx-background-color: #747474;" text="Log in" textFill="#272727">
